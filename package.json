{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir app/ --source-maps",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "npm run watch-compile",
    "fe-release": "export NODE_ENV=production&&node ./fe-xiaozhuye.js",
    "fe-dev": "export NODE_ENV=development&&node ./fe-xiaozhuye.js",
    "build": "bash rebuild.sh"
  },
  "dependencies": {
    "babel-runtime": "6.x.x",
    "jsonwebtoken": "^7.1.9",
    "source-map-support": "0.4.0",
    "thinkjs": "2.2.x"
  },
  "devDependencies": {
    "axios": "^0.15.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.25.0",
    "js-cookie": "^2.1.3",
    "lodash": "^4.16.4",
    "memcached": "^2.2.2",
    "node-crontab": "0.0.8",
    "nodemailer": "^2.6.4",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "request": "^2.76.0",
    "transliteration": "^1.1.11",
    "vue": "^2.0.3",
    "vue-datepicker": "^1.3.0",
    "vue-lazyload": "^0.8.3",
    "vue-loader": "^9.7.0",
    "vuex": "^2.0.0",
    "webpack": "^1.13.2"
  },
  "repository": "",
  "license": "MIT"
}
